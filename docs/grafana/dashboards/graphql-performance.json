      },
      "title": "Top 10 Slowest Operations",
      "type": "table",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "topk(10, sum by (operation_name) (rate(graphql_request_duration_seconds_sum[$__rate_interval])) / sum by (operation_name) (rate(graphql_request_duration_seconds_count[$__rate_interval])))",
          "format": "table",
          "instant": true,
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Time": true },
            "renameByName": { "Value": "Avg Duration", "operation_name": "Operation" }
          }
        }
      ]
